{
    "name": "serafim/stream",
    "description": "PHP streaming library",
    "type": "library",
    "license": "MIT",
    "keywords": ["stream", "restream", "handler", "protocol"],
    "support": {
        "issues": "https://github.com/SerafimArts/Stream/issues",
        "source": "https://github.com/SerafimArts/Stream"
    },
    "authors": [
        {
            "name": "Nesmeyanov Kirill",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": ">=7.4",
        "psr/simple-cache": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Serafim\\Stream\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": ">=4.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Serafim\\Stream\\Tests\\": "tests",
            "Mock\\": "tests/mocks"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
