{
    "name": "serafim/stream",
    "description": "PHP streaming library",
    "type": "library",
    "license": "MIT",
    "keywords": ["stream", "restream", "handler", "protocol"],
    "support": {
        "issues": "https://github.com/SerafimArts/Stream/issues",
        "source": "https://github.com/SerafimArts/Stream"
    },
    "authors": [
        {
            "name": "Nesmeyanov Kirill",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "psr/simple-cache": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "symfony/var-dumper": "~4.2"
    },
    "autoload": {
        "psr-4": {
            "Serafim\\Stream\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Serafim\\Stream\\Tests\\": "tests/",
            "Mock\\": "tests/mocks/"
        }
    }
}
